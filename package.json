{
  "name": "pokedex",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "start": "vite --open",
    "build": "vite build",
    "preview": "vite preview --open",
    "lint": "eslint . --cache -f table",
    "lint:fix": "eslint . --cache --fix",
    "spelling": "cspell \"**/*.{md,ts,tsx}\" --no-must-find-files -c .cspell.json",
    "format": "prettier --check \"src/**/*.{ts,tsx}\"",
    "format:fix": "prettier --write \"src/**/*.{ts,tsx}\"",
    "check": "npm run lint && npm run format && npm run spelling && npm run build"
  },
  "dependencies": {
    "@reduxjs/toolkit": "1.9.1",
    "leo-css-reset": "1.0.0",
    "nes.css": "2.3.0",
    "normalize.css": "8.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "8.0.5",
    "react-router-dom": "6.7.0",
    "rfs": "9.0.6"
  },
  "devDependencies": {
    "@types/node": "~18.11.18",
    "@types/react": "~18.0.27",
    "@types/react-dom": "~18.0.10",
    "@types/react-router-dom": "~5.3.3",
    "@typescript-eslint/eslint-plugin": "~5.49.0",
    "@typescript-eslint/parser": "~5.49.0",
    "@vitejs/plugin-react": "~3.0.1",
    "cspell": "~6.19.2",
    "eslint": "~8.32.0",
    "eslint-config-prettier": "~8.6.0",
    "eslint-formatter-table": "~7.32.1",
    "eslint-plugin-prettier": "~4.2.1",
    "gh-pages": "~5.0.0",
    "prettier": "~2.8.3",
    "sass": "~1.57.1",
    "stylelint": "~14.16.1",
    "stylelint-config-prettier": "~9.0.4",
    "stylelint-config-standard": "~29.0.0",
    "stylelint-config-standard-scss": "~6.1.0",
    "typescript": "~4.9.4",
    "vite": "~4.0.4",
    "vite-plugin-svgr": "~2.4.0"
  },
  "homepage": "https://borisvorobev.github.io/pokedex"
}
